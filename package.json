{
  "name": "@uchen/codeart-devenv",
  "version": "0.0.1",
  "description": "Utility to discover C++ development environment",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "dev:watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
    "test": "jest"
  },
  "keywords": [
    "c++",
    "devtools",
    "compiler"
  ],
  "author": "Eugene Ostroukhov <eostroukhov@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
